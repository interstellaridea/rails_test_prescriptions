<h1>All Projects</h1>
<table>
	<tr>
		<td>Project Name</td>
		<td>Total Projects Size</td>
	</tr>
	<tbody>
		<% @projects.each do |project| %>
		<tr class="project-row" id="<%= dom_id(project) %>">
			<td class="name">
				<%= project.name %>
			</td>
			<td class="total-size">
				<%= project.total_size %>
			</td>
		</tr>
		<% end %>
	</tbody>
</table>